#define DEFAULT 0
#define NAV     1
#define SYM     2
#define NUM     3
#define FUN     4
#define MEDIA   5
#define EMPTY	6

/ {
	keymap {
		compatible = "zmk,keymap";

		default_layer_0 {
			bindings = <
	&kp PSCRN	&kp Q		&kp W		&kp E		&kp R	&kp T																							&kp Y		&kp U		&kp I		&kp O		&kp P		&kp UP
	&kp TAB		&mt LSHIFT A &lt MEDIA S &lt NAV D	&kp F	&kp G																							&kp H		&kp J		&kp K		&kp L		&mt LSHIFT SEMI	&kp DOWN
	&kp LALT	&kp Z		&mt LCTRL X	&kp C		&kp V	&kp B	&kp BSPC		&kp RET									&kp LSHFT		&kp LSHFT		&kp N		&kp M		&kp COMMA	&mt LCTRL DOT &kp FSLH	&kp RIGHT
							&kp C_MUTE				&kp LCTRL		&kp ESC	&kp BSPC	&lt FUN TAB							&lt SYM RET		&lt NUM SPACE	&kp DEL		&kp LEFT	&none
			>;
			sensor-bindings = <&inc_dec_kp C_VOL_DN C_VOL_UP &inc_dec_kp C_VOL_DN C_VOL_UP>;
		};

		nav_layer_4 {
			bindings = <
	&none	&kp LS(DEL)	&kp LC(INS)	&none		&kp LS(INS)	&none																								&kp LC(Y)	&kp LS(INS)	&kp LC(INS)	&kp LS(DEL)	&kp LC(Z)	&none
