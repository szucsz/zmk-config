			>;
			sensor-bindings = <&inc_dec_kp C_VOL_DN C_VOL_UP &inc_dec_kp LC(LEFT) LC(RIGHT)>;
		};

		fun_layer_2 {
			bindings = <
	&none	&kp F12	&kp F7	&kp F8	&kp F9	&kp PSCRN																										&kp LC(Y)	&kp LS(INS)	&kp LC(INS)	&kp LS(DEL)	&kp LC(Z)	&none
